{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "rootDir": "./src/",
    "outDir": "./bin/",
    "strict": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "typeRoots": [
      "node_modules/@types",
      "typings",
      "node_modules/discord.js/typings",
      "node_modules/brain.js/dist/**/"
    ],
    "sourceMap": false,
    "baseUrl": "./"
  },
  "include": [
    "src",
    "typings"
  ],
}